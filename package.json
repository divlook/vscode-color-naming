{
	"name": "color-naming",
	"displayName": "color-naming",
	"description": "A VS Code Extension that names colors using ntc.js.",
	"license": "MIT",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.58.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:color-naming.helloWorld"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "color-naming.helloWorld",
				"title": "Hello World"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile && yarn run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.2.2",
		"@types/node": "14.x",
		"@types/vscode": "^1.58.0",
		"@typescript-eslint/eslint-plugin": "^4.26.0",
		"@typescript-eslint/parser": "^4.26.0",
		"change-case": "^4.1.2",
		"eslint": "^7.27.0",
		"eslint-plugin-prettier": "^3.4.0",
		"glob": "^7.1.7",
		"mocha": "^8.4.0",
		"prettier": "^2.3.2",
		"tsc-watch": "^4.4.0",
		"tscpaths": "^0.0.9",
		"typescript": "^4.3.2",
		"vscode-test": "^1.5.2"
	}
}
